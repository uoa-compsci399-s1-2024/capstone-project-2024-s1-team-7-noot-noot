{
  "expo": {
    "name": "sightsaver",
    "slug": "sightsaver",
    "version": "1.0.0",
    "owner": "sightsaver",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#f2f2f2"
      },
      "splash": {
        "image": "./assets/images/splash-light.png",
        "resizeMode": "contain",
        "backgroundColor": "#f2f2f2",
        "dark": {
          "image": "./assets/images/splash-dark.png",
          "resizeMode": "contain",
          "backgroundColor": "#404040"
        }
      },
      "package": "com.nootnoot.sightsaver",
      "versionCode": 1
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c283fa5f-f164-42d3-a896-f9feb25eae6c"
      }
    }
  }
}
